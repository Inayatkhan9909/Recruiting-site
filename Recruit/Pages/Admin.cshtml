@page
@model Recruit.Pages.AdminModel
@{
}




<table>
    <h1>List of Candidates for various vaccancies</h1>

    <thead>

       <th>Application no</th>
       <th>First Name</th>
       <th>Last Name</th>
       <th>Email</th>
       <th>Qualification</th>
       <th>Skills</th>
       <th>Applied for</th>

    </thead>
    <tbody>

        @foreach (var items in Model.applicantlist )
        {
            <tr>
                <td>@items.Form_no</td>
                <td>@items.firstname</td>
                <td>@items.lastname</td>
                <td>@items.email</td>
                <td>@items.qualification</td>
                <td>@items.skills</td>
                <td>@items.job</td>
                <td>@items.Created_at</td>

            </tr>


        }
       
       
      

    </tbody>



</table>